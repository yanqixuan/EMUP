Page{
    background-color: #eeeded;
    width: 100%;
    height: 100%;
    padding: 0;
    margin: 0;
}
.title{
    position: relative;
    width: 100%;
    background-color: #000000;
    height: 400rpx;
    margin-bottom: 20rpx;
}
.title-userinfo{
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
}
.title-vip{
    text-align: center;
    /* width: 380rpx; */
    border: 1px solid #FFC03A;
    color: #FFC03A;
    padding: 10rpx 20rpx;
}

.order{
    margin-bottom: 40rpx;
    width: 100%;
    background-color: #fff;
}
.myorder{
    padding: 0 20rpx;
    line-height: 90rpx;
    border-bottom: 4rpx solid #eeeded;
}
.myorder::after{
    content: '查看全部订单>';
    color: #bfbfbf;
    font-size: 75%;
    float: right;
}
.myorder-lists{
    display: flex;
}
.myorder-list{
    font-size: 60%;
    padding: 20rpx;
    text-align: center;
    flex: 1;
    /* width: 20rpx;
    height: 20rpx; */
}
.myorder-list image{
    width: 50rpx;
    height: 50rpx;
}

.other-lists{
    background-color: #fff;
    width: 100%;
    margin-bottom: 40rpx;
}
.other-list{
    /* padding: 20rpx; */
    padding: 10rpx 20rpx 10rpx 0;
    margin-left: 20rpx;
    line-height: 80rpx;
    height: 80rpx;
    border-bottom: 2rpx solid #eeeded;
}
.other-list::after{
    content: ">";
    color: #bfbfbf;
    float: right;
    vertical-align: middle;
}
.other-list-image{
    margin-right: 20rpx;
    vertical-align: middle;
}
.other-list image{
    width: 50rpx;
    height: 50rpx;
}
.other-list-title{
    display: inline;
    vertical-align: middle;
}